{
    "name": "egorov/yii2-annotations",
    "description": "Allows you to use doctrine/annotations in Yii2 projects",
    "type": "yii2-extension",
    "require": {
        "doctrine/annotations": "^1.6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "<7",
        "yiisoft/yii2-dev": "~2.0.14"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "d.egorov",
            "email": "desure85@gmail.ru"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": { "yii\\annotations\\": "src" }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
